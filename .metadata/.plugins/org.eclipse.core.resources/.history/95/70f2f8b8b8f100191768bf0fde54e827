
public class Uso_Array {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//otra forma de declararla
		//int mi_matriz[]= new int[5];
		
		int [] mi_matriz= new int[5];
		
		mi_matriz[0]=5;
		mi_matriz[1]=55;
		mi_matriz[2]=47;
		mi_matriz[3]=7;
		mi_matriz[4]=9;
		
		for(int i=0;i<5;i++) {
			
			System.out.println("valor de indece "+i+" es "+mi_matriz[i]);
		}
		

	}

}
