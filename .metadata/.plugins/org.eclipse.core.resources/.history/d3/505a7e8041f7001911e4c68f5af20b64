package poo;

public class Coche {

	private int ruedas;
	
	private int largo;
	
	private int ancho;
	
	private int  motor;
	
	private  int peso;
	
	
public Coche() {
		
		ruedas=4;
		largo=2000;
		ancho=300;
		motor=1600;
		peso=500;
		
	}

   //metodo getter

  public String dime_largo() {
	  
	  return "el largo del coche es " +largo;
  }

